    <New class="org.eclipse.jetty.plus.jndi.EnvEntry">
        <Arg>soa.sas.service.url</Arg>
        <Arg type="java.lang.String">${soa.sas.service.url}</Arg>
    </New>
    <New class="org.eclipse.jetty.plus.jndi.EnvEntry">
        <Arg>soa.ipm.service.url</Arg>
        <Arg type="java.lang.String">${soa.ipm.service.url}</Arg>
