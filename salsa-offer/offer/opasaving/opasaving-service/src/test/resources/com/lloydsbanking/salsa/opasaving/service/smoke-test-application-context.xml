<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <import resource="classpath:/com/lloydsbanking/salsa/configuration.xml"/>

    <bean id="rrbClient" class="com.lloydsbanking.salsa.rrb.client.RrbClientImpl">
        <constructor-arg value="${rrb.service.url}"/>
        <constructor-arg value="true"/>
    </bean>

    <bean id="testDataHelper" class="com.lloydsbanking.salsa.rrb.service.TestDataHelper"/>
</beans>
