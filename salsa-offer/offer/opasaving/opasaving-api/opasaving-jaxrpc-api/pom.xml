<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>opasaving-api</artifactId>
        <groupId>com.lloydsbanking.salsa</groupId>
        <version>0.0.2-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>opasaving-jaxrpc-api</artifactId>

    <properties>
        <generated-sources.ibm-wsdl-java>${project.build.directory}/generated-sources/ibm-wsdl-java
        </generated-sources.ibm-wsdl-java>
        <generated-java-root>${basedir}/target/generated-sources/ibm-wsdl-java</generated-java-root>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.ibm.ws.webservices</groupId>
            <artifactId>thinclient</artifactId>
            <version>6.1.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.lloydsbanking.salsa</groupId>
            <artifactId>common-jaxrpc-api</artifactId>
        </dependency>
        <dependency>
            <groupId>com.lloydsbanking.salsa</groupId>
            <artifactId>opasaving-schema</artifactId>
            <version>${project.parent.version}</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>add-source</goal>
                        </goals>
                        <configuration>
                            <sources>
                                <source>${generated-sources.ibm-wsdl-java}</source>
                            </sources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <id>wsdl2java</id>
                        <phase>generate-sources</phase>
                        <configuration>
                            <tasks>
                                <property name="project.build.directory"
                                          value="${project.build.directory}"/>
                                <property
                                        name="generated-sources.ibm-wsdl-java"
                                        value="${generated-sources.ibm-wsdl-java}"/>
                                <ant
                                        antfile="${basedir}/src/main/resources/build_wsdl2java.xml"
                                        target="doWSDLtoJava"/>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>

                    <!-- To Do Need to separate common schema for white zone XSDs -->
                     <execution>
                         <id>delete-common-files</id>
                         <phase>generate-sources</phase>
                         <configuration>
                             <target>
                                 <property name="generated-java-root" value="${generated-java-root}"/>
                                <ant antfile="${basedir}/../ant-utilities.xml"
                                     target="remove-generated-gbo-source-for-jaxrpc"/>
                             </target>
                         </configuration>
                         <goals>
                             <goal>run</goal>
                         </goals>
                     </execution>
                </executions>
                <dependencies>

                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>j2ee</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>runtime</artifactId>
                        <version>6.1.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>webcontainer</artifactId>
                        <version>2.0.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>portletcontainer</artifactId>
                        <version>2.0.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>ras</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>webservices</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>commons-discovery</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>commons-logging-api</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>qname</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>wsexception</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.etools.ejbdeploy.ejbquery</groupId>
                        <artifactId>querymd</artifactId>
                        <version>6.0.10</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.websphere.webservice.rt</groupId>
                        <artifactId>wsdl4j</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>ejb-command-target</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>wccm</artifactId>
                        <version>6.1.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>bootstrap</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws.plugins</groupId>
                        <artifactId>bootstrap</artifactId>
                        <version>6.1.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>emf</artifactId>
                        <version>2.1.0</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws</groupId>
                        <artifactId>server</artifactId>
                        <version>6.1.2</version>
                    </dependency>
                    <dependency>
                        <groupId>com.ibm.ws.jre.lib</groupId>
                        <artifactId>xml</artifactId>
                        <version>6.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.emf</groupId>
                        <artifactId>ecore</artifactId>
                        <version>2.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.emf</groupId>
                        <artifactId>common</artifactId>
                        <version>2.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.emf.ecore</groupId>
                        <artifactId>xmi</artifactId>
                        <version>2.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.equinox</groupId>
                        <artifactId>registry</artifactId>
                        <version>3.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.equinox</groupId>
                        <artifactId>common</artifactId>
                        <version>3.2.0</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.core</groupId>
                        <artifactId>runtime</artifactId>
                        <version>3.2.0</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.core</groupId>
                        <artifactId>runtime-compatibility</artifactId>
                        <version>3.1.100</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.core</groupId>
                        <artifactId>resources</artifactId>
                        <version>3.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.core</groupId>
                        <artifactId>jobs</artifactId>
                        <version>3.2.0</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse</groupId>
                        <artifactId>osgi</artifactId>
                        <version>3.2.1</version>
                    </dependency>
                    <dependency>
                        <groupId>org.apache.ant</groupId>
                        <artifactId>ant</artifactId>
                        <version>1.9.3</version>
                    </dependency>

                </dependencies>
            </plugin>
        </plugins>
    </build>


</project>