<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
        xmlns:bons0="http://LIB_SIM_GMO/Messages"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:bons1="http://LIB_SIM_BO/BusinessObjects"
        targetNamespace="http://LIB_SIM_SalesProcessManagement/Messages">


    <xsd:import
