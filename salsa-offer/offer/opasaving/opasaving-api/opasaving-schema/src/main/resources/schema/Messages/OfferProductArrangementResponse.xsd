    <xsd:import namespace="http://LIB_SIM_GMO/Messages"
                schemaLocation="../../../../../target/common-schema/schema/lib-sim-gmo/Messages/BaseResponse.xsd"/>

    <xsd:complexType name="OfferProductArrangementResponse">
        <xsd:complexContent>
            <xsd:extension base="bons0:BaseResponse">

                <xsd:sequence>
                    <xsd:element minOccurs="0" name="productArrangement"
